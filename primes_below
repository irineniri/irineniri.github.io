def is_prime(n):

    if n > 1:

        for i in range(2, int(n/2)+1):
    
            if (n % i) == 0:
                return False
               
        return True
  
    else:
         return False

def primes_below(z):

    a=[]

    for i in range(2,z):
        if is_prime(i):
            a.append(i)
    return(a)
